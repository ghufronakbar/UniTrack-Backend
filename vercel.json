{
  "version": 2,
  "crons": [
    {
      "path": "/api/cron/remind",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/test",
      "schedule": "*/5 * * * *"
    }
  ],
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}
